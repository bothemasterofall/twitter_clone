<% if signed_in? %>
	<table class="front" summary="For signed-in users">
		<tr>
			<td class="main">
				<h1 class="micropost">What's up?</h1>
				<%= render 'shared/micropost_form' %>
				<%= render 'shared/feed' %>
			</td>
			<td class="sidebar round">
				<%= render 'shared/user_info' %>
				<%= render 'shared/stats' %>
			</td>
		</tr>
	</table>
<% else %>
<% end %>
<h1>Home Page</h1>
<p>This is the home page <a href="http://railstutorial.org/"> on Rails application</p>

<%= link_to "Sign up now!", new_user_path, :class => "signup_button round" %>