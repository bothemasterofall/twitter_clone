<header>
	<nav class ="round">
		<ul>
			<li><%= link_to "Home", root_path %> </li>
			<li><%= link_to "Help",  pages_help_path %> </li>
			<% if signed_in? %>
				<li><%= link_to "Profile", user_path(current_user) %></li>
				<li><%= link_to "Sign out", destroy_session_path, method: :delete %></li>
				<li><%= link_to "Edit", edit_user_path(current_user)%></li>
			<% else %>
				<li><%= link_to "Sign in", new_session_path %>
			<% end %>
		</ul>
	</nav>
</header>